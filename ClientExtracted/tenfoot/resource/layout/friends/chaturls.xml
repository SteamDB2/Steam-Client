<root>
	<styles>
		<include src="file://{resources}/styles/steamstyles.css" />
		<include src="file://{resources}/styles/mainmenu.css" />
		<include src="file://{resources}/styles/msgbox.css" />
		<include src="file://{resources}/styles/friends/chaturls.css" />
	</styles>

	<TenfootMsgBox defaultfocus="Button0" tabindex="auto" selectionpos="auto">
		<Panel id="MsgBoxBodyRegion" class="MsgBoxBodyRegion">
			<Panel id="MsgBoxHeader" class="MsgBoxHeader">
				<Label id="MsgBoxTitle" class="MsgBoxTitle" text="#Friends_ChooseURL" />
			</Panel>
			<Panel id="MsgBoxButtonArea" class="MsgBoxButtonArea">
				<Panel id="MsgBoxButtonsVertical">	
				</Panel>

				<Panel id="MsgBoxButtons">	
					<Button id="ButtonCancel" class="ButtonCancel MsgBoxButton">
						<Label id="CancelButtonText" text="#Friends_ChooseURL_Cancel"/>
					</Button>
				</Panel>
				
			</Panel>
		</Panel>
	</TenfootMsgBox>
</root>
